<!doctype html>
<html lang="en">
<head>

    <!--  Essential META Tags -->

    {{#if post}}
        <meta name="description" content="Free, Open-source UI design: {{ description }}, designed with {{ software }}.">
        <meta name="keywords" content="UI Resources, UI Design, Open-Source Design, {{ software }} Design">
        <meta name="author" content="Ildiko Gaspar">

        <meta property="og:title" content="{{ title }}">
        <meta property="og:description" content="{{ description }}, designed with {{ software }}.">
        <meta property="og:image" content="https://uidesigndaily.com{{ image }}">
        <meta property="og:url" content="https://uidesigndaily.com/posts/{{ slug }}">

    {{else}}
        <meta name="description" content="Free, Open-source UI design resources updated daily.">
        <meta name="keywords" content="UI Resources, UI Design, Open-Source Design">
        <meta name="author" content="Ildiko Gaspar">

        <meta property="og:title" content="UI Design Daily">
        <meta property="og:description" content="Weekly FREE UI resources straight to your inbox.">
        <meta property="og:image" content="https://uidesigndaily.com/public/img/og-image.png">
        <meta property="og:url" content="https://uidesigndaily.com">

    {{/if}}

    <meta name="twitter:card" content="summary_large_image">


    <!--  Non-Essential, But Recommended -->

    <meta property="og:site_name" content="UI Design Daily">
    <meta name="twitter:image:alt" content="Weekly FREE UI resources straight to your inbox.">


    <!--  Non-Essential, But Required for Analytics -->

    <meta property="fb:app_id" content="uidesigndaily" />
    <meta name="twitter:site" content="@uidesigndaily">


    <title>{{ title }}</title>

    <meta charset="UTF-8">

    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">

    <link href="https://fonts.googleapis.com/css?family=Muli:400,700&display=swap" rel="stylesheet">


    <link rel="apple-touch-icon" sizes="180x180" href="/public/img/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/public/img/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/public/img/favicon-16x16.png">

    
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-159416330-1"></script>
    <script>
        if ( document.location.hostname.search("uidesigndaily.com") !== -1 ) {
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-159416330-1');
        }
    </script>

